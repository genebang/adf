<div class="container centered">
  <h2>Your Games</h2>

  <% @games.each do |game| %>
    <div class="game_<%=game.id%>">
      
      <%#= render 'matchup', :game => game %>
      <%= link_to render(:partial => 'matchup', :locals => {:game => game}), game_path(game) %>
    </div>
  <% end %>
</div>
